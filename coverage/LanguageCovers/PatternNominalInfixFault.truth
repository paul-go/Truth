//
// This example demonstrates a fault being generated due to
// an alias being used in the place where an actual type name
// is expected, as a result of the use of a nominal infix.
//

Number
Integer : Number
/\d+ : Integer

Text
Email : Text

Map
	Key : Number
	Val : Text

/Map\<<<Key>>; <<Val>>\> : Map

Class
	Good : Map<Integer, Email>
	Bad : Map<2; Email> #201;
